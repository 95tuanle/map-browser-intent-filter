<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/bt_geo_intent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Send Geo Intent"
        app:layout_constraintBottom_toTopOf="@+id/bt_web_intent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:onClick="sendGeoIntent"/>

    <Button
        android:id="@+id/bt_web_intent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Send Web Intent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bt_geo_intent"
        android:onClick="sendWebIntent"/>
</androidx.constraintlayout.widget.ConstraintLayout>